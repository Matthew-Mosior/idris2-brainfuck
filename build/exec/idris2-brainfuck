#!/bin/sh
# @generated by Idris 0.7.0-e3413689a, Chez backend

set -e # exit on any error

if [ "$(uname)" = Darwin ]; then
  DIR=$(zsh -c 'printf %s "$0:A:h"' "$0")
else
  DIR=$(dirname "$(readlink -f -- "$0")")
fi
export LD_LIBRARY_PATH="$DIR/idris2-brainfuck_app:$LD_LIBRARY_PATH"
export DYLD_LIBRARY_PATH="$DIR/idris2-brainfuck_app:$DYLD_LIBRARY_PATH"
export IDRIS2_INC_SRC="$DIR/idris2-brainfuck_app"

"$DIR/idris2-brainfuck_app/idris2-brainfuck.so" "$@"